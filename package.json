{
  "name": "panti-werdha-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm start",
    "server": "cd server && npm run dev",
    "start": "npm run dev",
    "build": "cd client && npm run build",
    "clean": "rimraf node_modules client/node_modules server/node_modules package-lock.json client/package-lock.json server/package-lock.json",
    "clean:install": "npm run clean && npm install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "nodemon": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "rimraf": "^5.0.5",
    "sqlite3": "^5.1.6"
  }
}
